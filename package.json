{
  "name": "fate-characters",
  "version": "1.0.0",
  "description": "primer proyecto de nodejs",
  "main": "index.html",
  "scripts": {
    "preinstall": "npm install -g grunt-cli && npm install -g sass npm install -g ruby && npm install --global gulp-cli",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss \" \"npm run dev \" ",
    "copy:fonts": "copyfiles -u 3 \"./node_modules/open-iconic/font/fonts/*.*\" \"dist\" ",
    "build:scss": "sass scss/ css/",
    "watch:scss": "onchange \"scss/*.scss\" -- npm run build:scss ",
    "clean": "rimraf dist",
    "imagemin": "imagemin images/* --out-dir dist/images && imagemin images/masters/* --out-dir dist/images/masters && imagemin images/servants/* --out-dir dist/images/servants",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin contact.html -d dist --htmlmin -o dist/contact.html && usemin params.html -d dist --htmlmin -o dist/params.html",
    "build": "npm run clean && npm run copy:fonts && npm run build:scss && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://JuanDavid24@bitbucket.org/JuanDavid24/fate-characters.git"
  },
  "author": "Jota",
  "license": "ISC",
  "homepage": "https://bitbucket.org/JuanDavid24/fate-characters#readme",
  "devDependencies": {
    "browser-sync": "^2.26.13",
    "copyfiles": "^2.4.1",
    "cssmin": "^0.4.3",
    "grunt": "^1.3.0",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-imagemin": "^4.0.0",
    "grunt-contrib-sass": "^2.0.0",
    "grunt-contrib-uglify": "^5.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-usemin": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-sass": "^5.1.0",
    "htmlmin": "0.0.7",
    "imagemin-cli": "^8.0.0",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.5.4",
    "onchange": "^7.1.0",
    "rimraf": "^3.0.2",
    "time-grunt": "^2.0.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "concurrently": "8.0.1",
    "jquery": "^3.5.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1",
    "sass": "^1.89.0"
  }
}
